{
    "name": "@comet/brevo-api",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/vivid-planet/comet-brevo-module/",
        "directory": "packages/api/"
    },
    "license": "BSD-2-Clause",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/*"
    ],
    "scripts": {
        "api-generator": "rimraf 'src/*/generated' && comet-api-generator generate",
        "clean": "rimraf lib",
        "build": "$npm_execpath run clean && tsc -p tsconfig.build.json",
        "dev": "tsc --watch --preserveWatchOutput -p tsconfig.build.json",
        "lint": "run-p lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/ package.json",
        "lint:tsc": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "peerDependencies": {
        "@comet/cms-api": "^5.3.0"
    },
    "dependencies": {
        "@mikro-orm/core": "^5.0.5",
        "@mikro-orm/nestjs": "^5.0.0",
        "@nestjs/graphql": "^10.0.0",
        "class-validator": "^0.13.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.47.0",
        "@azure/storage-blob": "^12.0.0",
        "@kubernetes/client-node": "^0.18.0",
        "class-transformer": "^0.5.0",
        "@comet/cms-api": "^5.3.0",
        "@mikro-orm/cli": "^5.7.1",
        "@comet/eslint-config": "^5.3.0",
        "@mikro-orm/postgresql": "^5.0.4",
        "pg-error-constants": "^1.0.0",
        "@mikro-orm/migrations": "^5.7.1",
        "nestjs-console": "^8.0.0",
        "express": "^4.17.1",
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@types/jest": "^29.5.0",
        "@types/rimraf": "^3.0.0",
        "@types/uuid": "^8.3.0",
        "eslint": "^8.0.0",
        "graphql": "^15.5.0",
        "jest": "^29.5.0",
        "jest-junit": "^15.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^7.0.0",
        "prettier": "^2.0.0",
        "@sendinblue/client": "^3.3.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.0.0",
        "typescript": "^4.0.0"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./src/db/ormconfig.cli.ts",
            "./dist/db/ormconfig.cli.js"
        ]
    }
}
